{
  "name": "unbrowse-mcp",
  "version": "1.1.10",
  "description": "The Internet-Use layer for AI Agents - MCP server providing 50x faster web interactions with 90%+ reliability",
  "main": "dist/stdio-server.cjs",
  "module": "./src/index.ts",
  "type": "commonjs",
  "bin": {
    "unbrowse-mcp": "bin/cli.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "npx smithery dev",
    "build:smithery": "npx smithery build",
    "build:stdio": "esbuild src/stdio-server.ts --bundle --platform=node --target=node18 --format=cjs --outfile=dist/stdio-server.cjs --external:@sentry/node",
    "build": "npm run build:stdio",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "api",
    "unbrowse",
    "llm",
    "claude",
    "anthropic",
    "ai-agents",
    "web-automation",
    "solana",
    "x402",
    "usdc",
    "pay-per-request"
  ],
  "author": "Unbrowse",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfoundry/unbrowse-mcp.git"
  },
  "homepage": "https://unbrowse.ai",
  "bugs": {
    "url": "https://github.com/getfoundry/unbrowse-mcp/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@sentry/node": "^10.22.0",
    "@smithery/sdk": "^1.7.4",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "bs58": "^6.0.0",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "undici": "^7.16.0",
    "zod": "^3.25.46"
  },
  "devDependencies": {
    "@smithery/cli": "^1.6.3",
    "esbuild": "^0.24.0"
  }
}
